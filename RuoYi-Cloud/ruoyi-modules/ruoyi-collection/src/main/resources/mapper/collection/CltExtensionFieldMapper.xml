<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.collection.mapper.CltExtensionFieldMapper">
  <resultMap type="CltExtensionField" id="CltExtensionFieldResult">
    <result property="extensionId" column="extension_id"/>
    <result property="examId" column="exam_id"/>
    <result property="testpaerId" column="testpaer_id"/>
    <result property="subjectCode" column="subject_code"/>
    <result property="extensionName" column="extension_name"/>
    <result property="extensionCode" column="extension_code"/>
    <result property="basicCodeTable" column="basic_code_table"/>
    <result property="extensionType" column="extension_type"/>
    <result property="belongType" column="belong_type"/>
    <result property="orderNum" column="order_num"/>
    <result property="remark" column="remark"/>
    <result property="status" column="status"/>
    <result property="delFlag" column="del_flag"/>
    <result property="createBy" column="create_by"/>
    <result property="createTime" column="create_time"/>
    <result property="updateBy" column="update_by"/>
    <result property="updateTime" column="update_time"/>
  </resultMap>

  <sql id="selectCltExtensionFieldVo">
        select extension_id, exam_id, testpaer_id, subject_code, extension_name, extension_code, basic_code_table, extension_type, belong_type, order_num, remark, status, del_flag, create_by, create_time, update_by, update_time from clt_extension_field
    </sql>

  <select id="selectCltExtensionFieldList" parameterType="CltExtensionField"
    resultMap="CltExtensionFieldResult">
    <include refid="selectCltExtensionFieldVo"/>
    <where>
      <if test="examId != null ">and exam_id = #{examId}
      </if>
      <if test="testpaerId != null ">and testpaer_id = #{testpaerId}
      </if>
      <if test="subjectCode != null  and subjectCode != ''">and subject_code = #{subjectCode}
      </if>
      <if test="extensionName != null  and extensionName != ''">and extension_name like concat('%',
        #{extensionName}, '%')
      </if>
      <if test="extensionCode != null  and extensionCode != ''">and extension_code like concat('%',
        #{extensionCode}, '%')
      </if>
    </where>
    order by order_num
  </select>


</mapper>
