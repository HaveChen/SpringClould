<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.collection.mapper.CltDataFieldMapper">
  <resultMap type="CltDataField" id="CltDataFieldResult">
    <result property="dataFieldId" column="data_field_id"/>
    <result property="dataTypeCode" column="data_type_code"/>
    <result property="fieldName" column="field_name"/>
    <result property="dataFieldName" column="data_field_name"/>
    <result property="defaultName" column="default_name"/>
    <result property="isExtend" column="is_extend"/>
    <result property="isNeed" column="is_need"/>
    <result property="baseTypeCode" column="base_type_code"/>
    <result property="orderNum" column="order_num"/>
    <result property="remark" column="remark"/>
    <result property="status" column="status"/>
    <result property="delFlag" column="del_flag"/>
    <result property="createBy" column="create_by"/>
    <result property="createTime" column="create_time"/>
    <result property="updateBy" column="update_by"/>
    <result property="updateTime" column="update_time"/>
  </resultMap>

  <sql id="selectCltDataFieldVo">
        select data_field_id, data_type_code, field_name, data_field_name, default_name, is_extend, is_need, base_type_code, order_num, remark, status, del_flag, create_by, create_time, update_by, update_time from clt_data_field
    </sql>

  <select id="selectCltDataFieldList" parameterType="CltDataField" resultMap="CltDataFieldResult">
    <include refid="selectCltDataFieldVo"/>
    <where>
      <if test="dataTypeCode != null  and dataTypeCode != ''">and data_type_code = #{dataTypeCode}
      </if>
    </where>
    order by order_num
  </select>


</mapper>
